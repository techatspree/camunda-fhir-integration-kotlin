          <zeebe:input source="20" target="readTimeoutInSeconds" />
        </zeebe:ioMapping>
        <zeebe:taskHeaders>
          <zeebe:header key="resultExpression" value="={&#10;  use: response.body.address[1].use,&#10;  street: string join(response.body.address[1].line), &#10;  city: response.body.address[1].city,&#10;  state: response.body.address[1].state,&#10;  zip: response.body.address[1].postalCode,&#10;  country: response.body.address[1].country&#10;}" />
          <zeebe:header key="retryBackoff" value="PT0S" />
        </zeebe:taskHeaders>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0vq3px0</bpmn:incoming>
      <bpmn:outgoing>Flow_0ubd5o9</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_0ubd5o9" sourceRef="getPatientData" targetRef="setPatientData" />
    <bpmn:serviceTask id="setPatientData" name="Send Patientdata to SAP">
